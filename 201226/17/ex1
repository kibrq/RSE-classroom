#!/usr/bin/env python3

''' Рассматривается множество целых чисел, принадлежащих отрезку [1390; 12567], которые делятся на 3 или на 5 и не делятся на 7, 11, 13 и 23. Найдите количество таких чисел и максимальное из них. В ответе запишите два числа через пробел: сначала количество, затем максимальное число. '''


def pred(n):
    ''' Проверяет условие задачи для заданного числа '''
    condition1 = n % 3 == 0 or n % 5 == 0
    condition2 = not (n % 7 == 0 or n % 11 == 0 or n % 13 == 0 or n % 23 == 0)
    # not (A or B) = (not A) and (not B)
    return condition1 and condition2


ans = [0, 0]  # Поддерживаемый ответ, например, пара <кол-во, сумма>


def handle(n):
    ans[0] += 1
    # можно просто ans[1] = n, так как перебираем n в порядке возрастания
    ans[1] = max(ans[1], n)


def answer():
    print(ans[0], ans[1])


def build():
    return range(1390, 12567 + 1)  # !!!!! [a, b + 1) == [a, b]


for n in build():
    if pred(n):
        handle(n)

answer()
